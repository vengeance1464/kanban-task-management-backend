// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model  User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  uuid      String
  name      String
  picture   String
  email     String
}


model Boards {
  id  String   @id @default(auto()) @map("_id") @db.ObjectId
  boardId String
  uuid  String
  name String
}

model Tasks {
      uuid String
      tasks
}